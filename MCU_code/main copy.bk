/*INTERRUPT HANDLER OF AFE4400*/
__attribute__((optimize("O2"))) 
void IRAM_ATTR AFE44X0_gpio_isr_handler(void* arg) 
{
    /*we do not process spi in isr,but in an outside task*/
    //BaseType_t xHigherPriorityTaskWoken = pdFALSE;
   // vTaskNotifyGiveFromISR(afe_task_handle, &xHigherPriorityTaskWoken);  
    //portYIELD_FROM_ISR(xHigherPriorityTaskWoken);
    AFE44x0_Reg_Read_dec_soft(s_spi_bus,0x2C,afe_4400rdbuff); //use delicated io based software spi
    afe_buffers_ext[afe_buf_head] = afe_4400rdbuff[1];
    afe_buf_head = (afe_buf_head+1)& 0xff;
    afe_buffers_ext[afe_buf_head] = afe_4400rdbuff[2];
    afe_buf_head = (afe_buf_head+1)& 0xff;
    afe_buffers_ext[afe_buf_head] = afe_4400rdbuff[3];
    afe_buf_head = (afe_buf_head+1)& 0xff;
    //afe_4400rdbuff_all[0] = afe_4400rdbuff[1];
    //afe_4400rdbuff_all[1] = afe_4400rdbuff[2];
    //afe_4400rdbuff_all[2] = afe_4400rdbuff[3];
    //memcpy(afe_4400rdbuff_all,afe_4400rdbuff+1,3);
    AFE44x0_Reg_Read_dec_soft(s_spi_bus,0x2A,afe_4400rdbuff);
    afe_buffers_ext[afe_buf_head] = afe_4400rdbuff[1];
    afe_buf_head = (afe_buf_head+1)& 0xff;
    afe_buffers_ext[afe_buf_head] = afe_4400rdbuff[2];
    afe_buf_head = (afe_buf_head+1)& 0xff;
    afe_buffers_ext[afe_buf_head] = afe_4400rdbuff[3];
    afe_buf_head = (afe_buf_head+1)& 0xff;
    //afe_4400rdbuff_all[3] = afe_4400rdbuff[1];
    //afe_4400rdbuff_all[4] = afe_4400rdbuff[2];
    //afe_4400rdbuff_all[5] = afe_4400rdbuff[3];
    //memcpy(afe_4400rdbuff_all+3,afe_4400rdbuff+1,3);
    //xRingbufferSendFromISR(afe_buffers,afe_4400rdbuff_all,6,0);
    afe_interrupt_val ++;
}

static volatile uint8_t curr_channel = 0;
static spi_transaction_t ads_rxtrans_32 = {.flags = SPI_TRANS_USE_RXDATA | SPI_TRANS_USE_TXDATA,.length = 32,.tx_data[0] = ADS_CMD_RDATA};
static spi_transaction_t ads_txtrans_16 = {.flags = SPI_TRANS_USE_TXDATA,.length = 16,.tx_data[0] = ADS_CMD_RREG_4};
static spi_transaction_t ads_txtrans_8 = {.flags = SPI_TRANS_USE_TXDATA,.length = 8,.tx_data[0] = ADS_CMD_START};
static volatile uint8_t ads_saved_data[12];
static volatile uint8_t ads_curr_channel_start;
/*INTERRUPT HANDLER OF ADS1220, by using spi task queue, return  at once*/
__attribute__((optimize("O2"))) 
void IRAM_ATTR ADS1220_gpio_isr_handler(void* arg)
{
        //read buffer from last transmisson
        ads_curr_channel_start = curr_channel*3;
        ads_saved_data[ads_curr_channel_start]      = ads_rxtrans_32.rx_data[1];
        ads_saved_data[ads_curr_channel_start+1]    = ads_rxtrans_32.rx_data[2];
        ads_saved_data[ads_curr_channel_start+2]    = ads_rxtrans_32.rx_data[3];
        //memcpy(&ads_saved_data[curr_channel*3],&(result[0].rx_data[1]),3); // copy to send data buffer
        //write data to ring buffer
        if(curr_channel == 3)
        {
            //witch is faster?
            
            int reamains = (ads_buf_head + 12) - RINGBUFFER_SIZE_T;
            if(reamains <  0)
            {
                memcpy(&ads_buffers_ext[ads_buf_head],ads_saved_data,12);
                ads_buf_head += 12;
            }
            else
            {
                uint16_t first_part = 12 - reamains;
                memcpy(&ads_buffers_ext[ads_buf_head],ads_saved_data,first_part);
                memcpy(ads_buffers_ext,&ads_saved_data[first_part],reamains);
                ads_buf_head = reamains;
            }
            
             //xRingbufferSendFromISR(ads_buffers,ads_saved_data,12,0);
        }
        curr_channel = (curr_channel == 3) ? 0: curr_channel + 1 ;
        ads_interrupt_val++;
        ads_txtrans_16.tx_data[1] = ((8 + curr_channel)<<4) | (0 <<1) | 0 ; 
        //finally send the next transaction quest
        spi_device_queue_trans(ads_spi,&ads_rxtrans_32,0);
        spi_device_queue_trans(ads_spi,&ads_txtrans_16,0);
        spi_device_queue_trans(ads_spi,&ads_txtrans_8,0);
}


