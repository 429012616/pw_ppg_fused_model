static uint8_t i2c_target = I2C_DEVICE_REG; //read from datax0
static volatile uint8_t i2c_recv_data[I2C_DEVICE_NBYTES];
/*INTERRUPT HANDLER OF ADXL345*/
__attribute__((optimize("O2"))) 
void IRAM_ATTR ADXL345_gpio_isr_handler(void* arg)
{
    //step1: read and write data to ringbuffer  from last trasmission
    adxl_buffers_ext[adxl_buf_head] = i2c_recv_data[0];
    adxl_buf_head = ((adxl_buf_head+1) & 0xff);
    adxl_buffers_ext[adxl_buf_head] = i2c_recv_data[1];
    adxl_buf_head = ((adxl_buf_head+1) & 0xff);
    adxl_buffers_ext[adxl_buf_head] = i2c_recv_data[2];
    adxl_buf_head = ((adxl_buf_head+1) & 0xff);
    adxl_buffers_ext[adxl_buf_head] = i2c_recv_data[3];
    adxl_buf_head = ((adxl_buf_head+1) & 0xff);
    adxl_buffers_ext[adxl_buf_head] = i2c_recv_data[4];
    adxl_buf_head = ((adxl_buf_head+1) & 0xff);
    adxl_buffers_ext[adxl_buf_head] = i2c_recv_data[5];
    adxl_buf_head = ((adxl_buf_head+1) & 0xff);
    //xRingbufferSendFromISR(adxl_buffers,i2c_recv_data,I2C_DEVICE_NBYTES,0);
    //step2: start another trasmission
    i2c_master_transmit_receive(i2c_dev_handle, (void *) &i2c_target, 1,
        (uint8_t*)i2c_recv_data, I2C_DEVICE_NBYTES, I2C_TIMEOUT_MS);
    adxl_interrupt_val++;
}
void AFE44x0_Reg_Write(spi_device_handle_t spi,unsigned char addr,unsigned long val)
{
    spi_transaction_t trans;
    memset(&trans,0,sizeof(spi_transaction_t));
    trans.length = 32;
    trans.user = (void*)0;
    trans.flags = SPI_TRANS_USE_TXDATA;
    trans.tx_data[0] = addr;
    trans.tx_data[1] = ((val & 0xffffff)>>16);
    trans.tx_data[2] = ((val & 0xffff)>>8);
    trans.tx_data[3] = (val & 0xff);
    esp_err_t ret = spi_device_transmit(spi, &trans);
    assert(ret == ESP_OK);
    vTaskDelay(1 / portTICK_PERIOD_MS);
}
void IRAM_ATTR AFE44x0_Reg_Write_dec_soft(soft_spi_bus_t spi,unsigned char addr,unsigned long val)
{
    esp_err_t ret;
    uint8_t wr_buffer[4];
    uint8_t rd_buffer[4];
    wr_buffer[0] = addr;
    wr_buffer[1] = (val & 0xffffff)>>16;
    wr_buffer[2] = ((val & 0xffff)>>8);
    wr_buffer[3] = (val & 0xff);
    ret = soft_spi_transfer(spi,wr_buffer,rd_buffer,4);
    assert(ret == ESP_OK);
}
