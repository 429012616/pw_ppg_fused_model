/*ads operations*/
void ADS1220_writeReg(spi_device_handle_t spi,spi_transaction_t* trans_reg, uint8_t addr,uint8_t val)
{
    trans_reg->tx_data[0] = ADS_CMD_WREG_4 | (addr<<2);
    trans_reg->tx_data[1] = val;
    spi_device_transmit(spi,trans_reg);
}
uint8_t ADS1220_readReg(spi_device_handle_t spi,spi_transaction_t* trans_reg, uint8_t addr)
{

    trans_reg->tx_data[0] = ADS_CMD_RREG_4 | (addr << 2);
    trans_reg->tx_data[1] = 0; //dummy bit
    spi_device_transmit(spi,trans_reg);
    return trans_reg->rx_data[1];   
}
void ADS1220_sendCMD(spi_device_handle_t spi,spi_transaction_t* trans_reg,uint8_t CMD)
{
    trans_reg -> tx_data[0] = CMD;
    spi_device_transmit(spi,trans_reg);
}
void ADS1220_initialization(spi_device_handle_t spi)
{
    spi_transaction_t trans_8 ={
        .length = 8,
        .flags = SPI_TRANS_USE_TXDATA
    };
    spi_transaction_t trans_16 ={
        .length = 16,
        .flags = SPI_TRANS_USE_TXDATA
    };    
    vTaskDelay(1 / portTICK_PERIOD_MS);
    ADS1220_sendCMD(spi,&trans_8,ADS_CMD_RESET);
    vTaskDelay(1 / portTICK_PERIOD_MS);
    ADS1220_writeReg(spi,&trans_16,0,0);                                //MUX channel[7:4], GAIN[3:1],PGA_BYPASS[0] (gain = 1, pga_bypass = 0)
    ADS1220_writeReg(spi,&trans_16,1,(6<<5)|(2<<3)|(0<<2)|(0<<1)|(0));  //DR[7:5],MODE[4:3],CM[2],TS[1],BCS[0] ï¼ˆ2000sps,turbo mode single shot)
    ADS1220_writeReg(spi,&trans_16,2,(0<<6)|(1<<4)|(0<<3)|(0));         //VREF[7:6],50/60FULTER[5:0=4],PSW[3],IDAC[2:0];(internal vref,50Hz/60Hz-rejection)
    ADS1220_writeReg(spi,&trans_16,3,1<<1);                                //(dont use IDAC, drdy mode set to when data is ready)
    ADS1220_sendCMD(spi,&trans_8,ADS_CMD_START);
}

void ADXL345_initialization(i2c_master_dev_handle_t dev_handle)
{

    volatile uint8_t data_wr[2] = {0};
    volatile uint8_t data_rd = 0;
    volatile esp_err_t i2c_err;
    //test device id first since last transaction may not complete, leading to error result.
    while(1)
    {

        data_wr[0] = 0;
        data_rd = 0;
        //test device id reading first
        i2c_err = i2c_master_transmit_receive(dev_handle, &data_wr[0], 1, &data_rd, 1, 0);
        ESP_ERROR_CHECK(i2c_err);
        vTaskDelay(pdMS_TO_TICKS(10)); // wait for transaction complete
        ESP_LOGI("ADXL345","ADXL_ADDRESS_TEST: 0x%02X",data_rd);
        if(data_rd == 0xE5)
        {
            ESP_LOGI("ADXL345","Device DEVID read PASSED");
            break;
        }
        else
        {
            ESP_LOGI("ADXL345","Device DEVID read FAILED,try to read again");
        }
        vTaskDelay(pdMS_TO_TICKS(10));
    }
    while(1)
    {
        data_wr[0] = ADXL_POWER_CTL;
        data_wr[1] = 0x4;//set to sleep mode first
        i2c_err = i2c_master_transmit(dev_handle, data_wr, 2, 100);
        ESP_ERROR_CHECK(i2c_err);
        vTaskDelay(pdMS_TO_TICKS(10));

        data_wr[0] = ADXL_INT_ENABLE;
        data_wr[1] = 0x00; //disabling all interrupt setting first
        i2c_err = i2c_master_transmit(dev_handle, data_wr, 2, 100);
        ESP_ERROR_CHECK(i2c_err);
        vTaskDelay(pdMS_TO_TICKS(10));

        data_wr[0] = ADXL_DATA_FORMAT;
        data_wr[1] = 0x2;//set range to 8 g
        i2c_err = i2c_master_transmit(dev_handle, data_wr, 2, 100);
        ESP_ERROR_CHECK(i2c_err);
        vTaskDelay(pdMS_TO_TICKS(10));

        data_wr[0] = ADXL_BW_RATE;
        data_wr[1] = 0x0D;//800,lets see if these can work under these i2c setting <- 1600 hz at turbo 400khz mode is fine
        i2c_err = i2c_master_transmit(dev_handle, data_wr, 2, 100);
        ESP_ERROR_CHECK(i2c_err);
        vTaskDelay(pdMS_TO_TICKS(10));

        data_wr[0] = ADXL_INT_MAP;
        data_wr[1] = 0x7F; //set all other to int2, set data ready to int1
        i2c_err = i2c_master_transmit(dev_handle, data_wr, 2, 100);
        ESP_ERROR_CHECK(i2c_err);
        vTaskDelay(pdMS_TO_TICKS(10));

        data_wr[0] = ADXL_INT_ENABLE;
        data_wr[1] = 0x80; //enbale data_ready interrupt ?????? WHY SET THIS TO 0X00 LEAD CORRECT RESULT ??? SUPPOSE TO BE 0X80 <- problem sovled,the adxl handler task is triggered by twice
        i2c_err = i2c_master_transmit(dev_handle, data_wr, 2, 100);
        ESP_ERROR_CHECK(i2c_err);
        vTaskDelay(pdMS_TO_TICKS(10));


        data_wr[0] = ADXL_POWER_CTL;
        data_wr[1] = 0x8;//set to measure mode
        i2c_err = i2c_master_transmit(dev_handle, data_wr, 2, 100);
        ESP_ERROR_CHECK(i2c_err);
        vTaskDelay(pdMS_TO_TICKS(10));

        data_wr[0] = 0;
        data_rd = 0;
        //test device id reading first
        i2c_err = i2c_master_transmit_receive(dev_handle, &data_wr[0], 1, &data_rd, 1, 100);
        ESP_ERROR_CHECK(i2c_err);
        vTaskDelay(pdMS_TO_TICKS(10)); // wait for transaction complete
        ESP_LOGI("ADXL345","ADXL_ADDRESS_TEST: 0x%02X",data_rd);
        if(data_rd == 0xE5)
        {
            ESP_LOGI("ADXL345","Device DEVID read PASSED");
            break;
        }
        else
        {
            ESP_LOGI("ADXL345","Device DEVID read FAILED,try to reconfiguration");
        }
        vTaskDelay(pdMS_TO_TICKS(10));
    }
}

